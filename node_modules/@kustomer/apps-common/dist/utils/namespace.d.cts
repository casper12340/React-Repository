type NamespaceOptions = {
    /** The name of the app */
    readonly app: string;
    /** Whether or not to namespace the components of the app */
    readonly namespace?: boolean;
};
/**
 * Function to namespace component names
 * @param name The name of the component
 * @param options The options for the namespace
 * @returns The namespaced component name
 */
declare function namespace(name: string, options: NamespaceOptions): string;

export { NamespaceOptions, namespace };
